package dao.headerMenu;

public class HeaderSubMenuDTO {

	private int id;
	private int menuId;
	private String name;
	private int idx;
	private boolean highlight;
	private String korName;

	public HeaderSubMenuDTO() {
	}

	public HeaderSubMenuDTO(int id, int menuId, String name, int idx,boolean highlight, String korName) {
		this.id = id;
		this.menuId = menuId;
		this.name = name;
		this.idx = idx;
		this.highlight = highlight;
		this.korName = korName;
	}

}