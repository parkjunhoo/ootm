package dao.headerMenu;

import java.util.ArrayList;

public class HeaderMenuDTO {
	private int id;
	private String name;
	private int idx;
	private ArrayList<HeaderSubMenuDTO> subMenu;
	
	public HeaderMenuDTO() {
	}
	
	public HeaderMenuDTO(int id ,String name, ArrayList<HeaderSubMenuDTO> subMenu) {
		this.id = id;
		this.name = name;
		this.subMenu = subMenu;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public ArrayList<HeaderSubMenuDTO> getSubMenu() {
		return subMenu;
	}

	public void setSubMenu(ArrayList<HeaderSubMenuDTO> subMenu) {
		this.subMenu = subMenu;
	}
	
	
}